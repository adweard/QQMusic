{
  "app": {
    "signingConfigs": [],
    "products": [
      {
        "name": "default",
        "signingConfig": "default",
        "compatibleSdkVersion": "5.0.0(12)",
        "runtimeOS": "HarmonyOS",
        "buildOption": {
          "strictMode": {
            "caseSensitiveCheck": true,
            "useNormalizedOHMUrl": true
          }
        }
      }
    ],
    "buildModeSet": [
      {
        "name": "debug",
      },
      {
        "name": "release"
      }
    ]
  },
  "modules": [
    {
      "name": "entry",
      "srcPath": "./products/entry",
      "targets": [
        {
          "name": "default",
          "applyToProducts": [
            "default"
          ]
        }
      ]
    },
    {
      "name": "recommend",
      "srcPath": "./features/recommend",
    },
    {
      "name": "find",
      "srcPath": "./features/find",
    },
    {
      "name": "moments",
      "srcPath": "./features/moments",
    },
    {
      "name": "mine",
      "srcPath": "./features/mine",
    },
    {
      "name": "radar",
      "srcPath": "./features/radar",
    }
  ]
}